//Question : Try Java Synchronized Method.
class TrySynchronization {
	synchronized void printTable(int n){//synchronized method  
		   for(int i=1;i<=5;i++){  
		     System.out.println(n*i);  
		     try{  
		      Thread.sleep(400);  
		     }catch(Exception e){System.out.println(e);}  
		   }  
		  
		 }  
		}  
		  class MyThread1 extends Thread{  
		TrySynchronization t;  
		MyThread1(TrySynchronization t){  
		this.t=t;  
		}  
		public void run(){  
		t.printTable(5);  
		}  
		  
		}  
		class MyThread2 extends Thread{  
			TrySynchronization t;  
		MyThread2(TrySynchronization t){  
		this.t=t;  
		}  
		public void run(){  
		t.printTable(100);  
		}  
		}  
		  
		public class Synchronization{  
		public static void main(String args[]){  
			TrySynchronization obj = new TrySynchronization();//only one object  
		MyThread1 t1=new MyThread1(obj);  
		MyThread2 t2=new MyThread2(obj);  
		t1.start();  
		t2.start();  
		}  
		}  
